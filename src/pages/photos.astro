---
import { getEntry } from "astro:content";
import PhotoLayout from "../layouts/PhotoLayout.astro";
import Category from "../components/Category.astro";
const monoCategoryPhoto = await getEntry("photos", "dock");
const colorCategoryPhoto = await getEntry("photos", "shipwrecked");
---

<PhotoLayout title="Photos">
  <ul
    class="grid grid-cols-2 md:grid-cols-3 lg:grid-cols-4 xl:grid-cols-5 gap-2 p-0"
  >
    <Category entry={monoCategoryPhoto} />
    <Category entry={colorCategoryPhoto} />
  </ul>
</PhotoLayout>
