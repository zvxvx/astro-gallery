---
import PhotoLayout from "../../../layouts/PhotoLayout.astro";
import { getCollection } from "astro:content";
import Photograph from "../../../components/Photograph.astro";
export const getStaticPaths = async () => {
  const color = await getCollection("color");
  return color.map((p) => ({
    params: { slug: p.data.slug },
    props: { p },
  }));
};
const {
  p: {
    collection,
    data: { title, date, image },
  },
} = Astro.props;
---

<PhotoLayout title={title}>
  <Photograph
    image={image}
    title={title}
    date={date}
    categoryPath={collection}
  />
</PhotoLayout>
